@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace dOSC.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link href="dOSC.styles.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/css/bootstrap.min.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/css/icons.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/css/fonts/lucide.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/dOSCEngine.styles.css" rel="stylesheet" />
    <link href="_content/Z.Blazor.Diagrams/style.min.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/style.min.css" rel="stylesheet" />
    <link href="_content/dOSCEngine/css/main.min.css" rel="stylesheet" />
    <link href="BlazorServer.styles.css" rel="stylesheet" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="_content/Z.Blazor.Diagrams/script.min.js"></script>
    <script src="_content/dOSCEngine/js/jquery-3.7.1.min.js"></script>
    <script src="_content/dOSCEngine/js/bootstrap.bundle.min.js"></script>
    <script src="_content/dOSCEngine/js/dOSC.js"></script>
</body>
</html>
